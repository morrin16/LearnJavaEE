<?xml version="1.0" encoding="UTF-8"?>
<web-app
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
    version="3.1">

    <display-name>Dynamic Web Application</display-name>

    <welcome-file-list>
        <welcome-file>lesson3_QE.html</welcome-file>
        <welcome-file>lesson1_Hello.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>lesson1.HelloServlet</servlet-class>
        <init-param>
            <param-name>First Init Parameter</param-name>
            <param-value>First Init Parameter Value</param-value>
        </init-param>
        <init-param>
            <param-name>Second Init Parameter</param-name>
            <param-value>Second Init Parameter Value</param-value>
        </init-param>
        <init-param>
            <param-name>Third Init Parameter</param-name>
            <param-value>Third Init Parameter Value</param-value>
        </init-param>
    </servlet>

</web-app>
